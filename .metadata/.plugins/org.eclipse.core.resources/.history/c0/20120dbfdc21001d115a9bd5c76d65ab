int main(void) {
	int num1,num2,num3;
printf("enter the 3 number");
scanf("%d%d%d",&num1,&num2,&num3);
if(num1>num2){
	printf("the number is grt");

	if (num2>num3){
		printf("the number is grt");


	}
	else{
		printf("the number is grt");
	}

}

else{
	printf("the number is grt");
}
	return EXIT_SUCCESS;
}
