#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num1,num2;
printf("enter the number");
scanf("%d%d",&num1,&num2);
if(num1>num2){
	printf("the number is great");
}
else{
	printf("the number not great");

	return EXIT_SUCCESS;
}
